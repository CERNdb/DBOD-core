package DBOD;

use strict;
use warnings;

use Carp;

use DBOD::Job;
use Data::Dumper;

# Initiates logger
BEGIN { 
    Log::Log4perl->easy_init() ;
}
my $job = DBOD::Job->new_with_options();

sub body {
    my $params = shift;
    my @time = $job->db->select('select now()',);
    $job->db->do('select pg_sleep(10)');
    @time = $job->db->select('select now()');
    return 0;
}

$job->run(\&body);

